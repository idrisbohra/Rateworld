<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" 
    android:background="#fff">

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/sb_md"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/ImageView01"
        android:paddingBottom="20dp"
        android:paddingTop="20dp"
        android:text="Notification"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:textSize="18sp"
        android:textStyle="bold" />

    <com.rateworld.switchbutton.SwitchButton
        android:id="@+id/sb_md"
        android:layout_width="wrap_content"
		android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="90dp"
        android:paddingRight="15dp"
        android:paddingTop="20dp"
       	android:paddingBottom="20dp"
        android:layout_toRightOf="@+id/textView1"
         />

   <View
       android:id="@+id/View10"
       android:layout_width="wrap_content"
       android:layout_height="1dp"
       android:layout_alignLeft="@+id/textView1"
       android:layout_alignRight="@+id/sb_md"
       android:layout_below="@+id/textView1"
       
       android:background="#4c4c4c" />

   <TextView
       android:id="@+id/TextView01"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:layout_alignLeft="@+id/View10"
       android:layout_below="@+id/View10"
       android:paddingBottom="20dp"
       android:paddingTop="20dp"
       android:text="Easy to Use"
       android:textAppearance="?android:attr/textAppearanceSmall"
       android:textSize="18sp"
       android:textStyle="bold" />

   <com.rateworld.switchbutton.SwitchButton
       android:id="@+id/SwitchbButton01"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:layout_alignBottom="@+id/TextView01"
       android:paddingRight="15dp"
       android:paddingTop="20dp"
       android:paddingBottom="20dp"
       android:layout_alignRight="@+id/View10"
        />

   <View
       android:id="@+id/View01"
       android:layout_width="wrap_content"
       android:layout_height="1dp"
       android:layout_alignLeft="@+id/TextView01"
       android:layout_alignRight="@+id/SwitchbButton01"
       android:layout_below="@+id/TextView01"
       
       android:background="#4c4c4c" />

   <TextView
       android:id="@+id/TextView02"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:layout_alignLeft="@+id/View01"
       android:layout_below="@+id/View01"
       android:paddingBottom="20dp"
       android:paddingTop="20dp"
       android:text="About Us"
       android:textAppearance="?android:attr/textAppearanceSmall"
       android:textSize="18sp"
       android:textStyle="bold" />

   <ImageView
       android:id="@+id/imageView1"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:layout_alignBottom="@+id/TextView02"
       android:layout_alignLeft="@+id/ImageView01"
       android:layout_below="@+id/View01"
       android:layout_toLeftOf="@+id/TextView02"
       android:src="@drawable/ic_launcher16" />

   <ImageView
       android:id="@+id/ImageView01"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:layout_alignBottom="@+id/textView1"
       android:layout_alignParentLeft="true"
       android:layout_alignParentTop="true"
       android:layout_marginLeft="5dp"
       android:src="@drawable/ic_launcher15" />

   <ImageView
       android:id="@+id/ImageView02"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:layout_above="@+id/TextView02"
       android:layout_alignLeft="@+id/ImageView01"
       android:layout_alignRight="@+id/imageView1"
       android:layout_alignTop="@+id/TextView01"
       android:src="@drawable/ic_launcher17" />

   <ImageView
       android:id="@+id/imageView2"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:layout_above="@+id/TextView02"
       android:layout_alignLeft="@+id/ImageView01"
       android:layout_alignRight="@+id/imageView1"
       android:layout_alignTop="@+id/TextView01"
       android:visibility="gone"
       android:src="@drawable/ic_launcher18" />

   <ImageView
       android:id="@+id/imageView3"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:layout_alignBottom="@+id/textView1"
       android:layout_alignParentLeft="true"
       android:layout_alignParentTop="true"
       android:layout_marginLeft="5dp"
       android:visibility="gone"
       android:src="@drawable/ic_launcher19" />

</RelativeLayout>
